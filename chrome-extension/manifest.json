{
  "manifest_version": 3,
  "name": "FB Extension",
  "description": "FB Extension",
  "version": "1.0",
  "permissions": [
    "webNavigation"
  ],
  "icons": {
    "16": "img/fb-16.png",
    "32": "img/fb-32.png",
    "48": "img/fb-48.png",
    "128": "img/fb-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "img/fb-16.png"
  },
  "content_scripts": [
    {
      "js": ["js/content.js"],
      "matches": ["*://*.facebook.com/*"]
    }
  ]
}